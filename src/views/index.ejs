        <!-- Load jquery -->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js" defer></script>
        <!-- load Scripts -->
        <script src="js/main.js" defer></script>
        
    </head>
    <body>
        <header>
            <img src="/images/logo.JPG" alt="logo" id="logo" />
        </header>
        <main>
            <div class="searchSection">
                <h1 class='promt_text'>Where are you going next?</h1>
                <div class='text_input'>
                    <input type="text" id="city_input" />
                    <button id="search_btn">Search</button> 
                </div>
            </div>
            <span class='alert'>Bad Request</span>
            <div id='mapdiv'>
                
            </div>
        </main>
        <footer>
                <div id='footer' class='col-md-12'>
                    <h4 class='text-center'>&copy;2021 <a href='https://nefi-aguilar-portfolio.herokuapp.com/'>Nefi Aguilar</a> </h4>
                </div>
        </footer>
    </body>
</html>
